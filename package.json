{
  "name": "relational-playground",
  "author": "Michael Mior <mmior@mail.rit.edu>",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://relationalplayground.com/",
  "repository": "https://github.com/dataunitylab/relational-playground",
  "bugs": "https://github.com/dataunitylab/relational-playground/issues",
  "dependencies": {
    "@michaelmior/js-sql-parser": "^1.3.0",
    "@sentry/browser": "^10.33.0",
    "@sentry/cli": "^3.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "fast-equals": "^6.0.0",
    "fromentries": "^1.3.2",
    "history": "^5.3.0",
    "immer": "^11.1.3",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-cookie": "^8.0.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-joyride": "^2.9.3",
    "react-redux": "^9.2.0",
    "react-router": "^7.13.1",
    "react-scripts": "5.0.1",
    "react-simple-code-editor": "^0.14.1",
    "react-simple-tree-menu": "^1.1.18",
    "react-split-pane": "^0.1.92",
    "react-table": "^6.11.5",
    "redux": "^4.2.1",
    "redux-first-history": "^5.2.0",
    "tinyqueue": "^2.0.3",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "build": "react-scripts build",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token \"$CHROMATIC_PROJECT_TOKEN\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "eject": "react-scripts eject",
    "flow": "flow check src",
    "lint": "prettier --check src/**/*.{js,json} && eslint --ext .js,.yml . && npmPkgJsonLint -q . && pkg-ok && markdownlint *.md",
    "netlify-build": "REACT_APP_GIT_SHA=`([ -n \"$COMMIT_REF\" ] && echo \"$COMMIT_REF\") || git rev-parse HEAD` react-scripts build && storybook build -o build/storybook/ && esdoc && mv docs/ build/",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "start": "react-scripts start",
    "storybook": "storybook dev -p 9009",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@chromatic-com/storybook": "^4.1.3",
    "@sebastianwessel/esdoc": "^2.1.0",
    "@sebastianwessel/esdoc-ecmascript-proposal-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-flow-type-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-jsx-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-react-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-standard-plugin": "^2.1.0",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/node-logger": "^8.6.14",
    "@storybook/preset-create-react-app": "^10.1.11",
    "@storybook/react": "^10.1.11",
    "@storybook/react-webpack5": "^10.1.11",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "bundlewatch": "^0.4.1",
    "coveralls": "^3.1.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-yaml": "^1.1.3",
    "flow-bin": "^0.203.1",
    "husky": "^9.1.7",
    "jest-each": "^30.2.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.47.0",
    "npm-package-json-lint": "^9.1.0",
    "npm-package-json-lint-config-default": "^8.0.1",
    "pkg-ok": "^3.0.0",
    "prettier": "^3.7.4",
    "redux-mock-store": "^1.5.5",
    "source-map-explorer": "^2.5.3",
    "storybook": "^10.2.10",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "universal-cookie": "^8.0.1",
    "webpack": "^5.104.1"
  },
  "engines": {
    "node": "24.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.stories.js",
      "!src/index.js"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "build/static/js/main.*.js",
        "maxSize": "200 kB"
      },
      {
        "path": "build/static/css/main.*.css",
        "maxSize": "5 kB"
      }
    ]
  },
  "pnpm": {
    "overrides": {
      "jest-environment-jsdom": "27.4.6"
    }
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
