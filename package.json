{
  "name": "relational-playground",
  "author": "Michael Mior <mmior@cs.rit.edu>",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://relational-playground.surge.sh/",
  "dependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "connected-react-router": "^6.5.2",
    "fromentries": "^1.2.0",
    "history": "^4.7.2",
    "js-sql-parser": "^1.2.0",
    "prop-types": "^15.5.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-select": "^3.0.4",
    "react-split-pane": "^0.1.87",
    "react-table": "^6.10.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "*"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "flow": "flow check src",
    "lint": "node_modules/.bin/prettier --check src/**/*.{js,jsx,json} && eslint src",
    "now-build": "react-scripts build && build-storybook -o build/storybook/ && esdoc && mv docs/ build/",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/cli": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/react": "^9.1.1",
    "babel-loader": "8.0.6",
    "coveralls": "^3.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "flow-bin": "^0.105.2",
    "husky": "^3.0.3",
    "jest-enzyme": "^7.1.0",
    "lint-staged": "^9.2.3",
    "npm-package-json-lint": "^3.7.0",
    "npm-package-json-lint-config-default": "^2.0.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "source-map-explorer": "^2.0.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "flow focus-check",
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npmPkgJsonLint -q .",
      "pkg-ok"
    ]
  }
}
