{
  "name": "relational-playground",
  "author": "Michael Mior <mmior@cs.rit.edu>",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://relationalplayground.com/",
  "repository": "https://github.com/michaelmior/relational-playground",
  "bugs": "https://github.com/michaelmior/relational-playground/issues",
  "dependencies": {
    "@michaelmior/js-sql-parser": "^1.3.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@sentry/browser": "^7.7.0",
    "@sentry/cli": "^2.5.0",
    "@testing-library/jest-dom": "^5.16.4",
    "connected-react-router": "^6.9.3",
    "fast-equals": "^4.0.1",
    "fromentries": "^1.3.2",
    "history": "^4.10.1",
    "immer": "^9.0.15",
    "prismjs": "^1.28.0",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.1",
    "react-joyride": "^2.5.0",
    "react-redux": "^7.2.8",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-simple-code-editor": "^0.11.2",
    "react-simple-tree-menu": "^1.1.18",
    "react-split-pane": "^0.1.92",
    "react-table": "^6.11.5",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "typescript": "*",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token \"$CHROMATIC_PROJECT_TOKEN\"",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "flow": "flow check src",
    "lint": "node_modules/.bin/prettier --check src/**/*.{js,json} && eslint --ext .js,.yml . && npmPkgJsonLint -q . && pkg-ok && markdownlint *.md",
    "postinstall": "husky install",
    "report-deploy": "bash ./scripts/report-deploy.sh",
    "snapshot": "build-storybook",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test",
    "vercel-build": "REACT_APP_GIT_SHA=`([ -n \"$VERCEL_GIT_COMMIT_SHA\" ] && echo \"$VERCEL_GIT_COMMIT_SHA\") || git rev-parse HEAD` react-scripts build && build-storybook -o build/storybook/ && esdoc && mv docs/ build/ && yarn report-deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@sebastianwessel/esdoc": "^2.1.0",
    "@sebastianwessel/esdoc-ecmascript-proposal-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-flow-type-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-jsx-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-react-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-standard-plugin": "^2.1.0",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.1.21",
    "@testing-library/react": "^12.1.5",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "bundlewatch": "^0.3.3",
    "chromatic": "^6.7.2",
    "coveralls": "^3.1.1",
    "enzyme": "^3.11.0",
    "eslint-plugin-yaml": "^0.5.0",
    "flow-bin": "^0.181.2",
    "husky": "8",
    "jest-each": "^28.1.3",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "pkg-ok": "^3.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0"
  },
  "engines": {
    "node": "14.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.stories.js",
      "!src/index.js"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "build/static/js/2.*.chunk.js",
        "maxSize": "200 kB"
      },
      {
        "path": "build/static/js/main.*.chunk.js",
        "maxSize": "10 kB"
      },
      {
        "path": "build/static/js/runtime-main.*.js",
        "maxSize": "1 kB"
      }
      ]
  }
}
