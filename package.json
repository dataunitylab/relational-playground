{
  "name": "relational-playground",
  "author": "Michael Mior <mmior@cs.rit.edu>",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://relational-playground.surge.sh/",
  "dependencies": {
    "connected-react-router": "^6.4.0",
    "fromentries": "^1.1.0",
    "js-sql-parser": "^1.0.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-select": "^2.1.1",
    "react-split-pane": "^0.1.84",
    "react-table": "^6.8.6",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "flow": "flow check src",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/cli": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "flow-bin": "^0.98.1",
    "husky": "^2.3.0",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.0.4",
    "npm-package-json-lint": "^3.4.1",
    "npm-package-json-lint-config-default": "^2.0.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3",
    "react-testing-library": "^7.0.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "flow focus-check",
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npmPkgJsonLint -q .",
      "pkg-ok"
    ]
  }
}
