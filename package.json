{
  "name": "relational-playground",
  "author": "Michael Mior <mmior@cs.rit.edu>",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://relationalplayground.com/",
  "repository": "https://github.com/michaelmior/relational-playground",
  "bugs": "https://github.com/michaelmior/relational-playground/issues",
  "dependencies": {
    "@michaelmior/js-sql-parser": "^1.3.0",
    "@sentry/browser": "^5.19.2",
    "@sentry/cli": "^1.55.0",
    "@testing-library/jest-dom": "^5.11.0",
    "connected-react-router": "^6.6.1",
    "fast-equals": "^2.0.0",
    "fromentries": "^1.2.0",
    "history": "^4.10.1",
    "immer": "^7.0.5",
    "prismjs": "^1.20.0",
    "prop-types": "^15.5.0",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-joyride": "^2.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-simple-code-editor": "^0.11.0",
    "react-split-pane": "^0.1.91",
    "react-table": "^6.11.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "*",
    "universal-cookie": "latest"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "flow": "flow check src",
    "lint": "node_modules/.bin/prettier --check src/**/*.{js,json} && eslint src && npmPkgJsonLint -q . && pkg-ok && markdownlint *.md",
    "now-build": "REACT_APP_GIT_SHA=`([ -n \"$NOW_GITHUB_COMMIT_SHA\" ] && echo \"$NOW_GITHUB_COMMIT_SHA\") || git rev-parse HEAD` react-scripts build && build-storybook -o build/storybook/ && esdoc && mv docs/ build/ && yarn report-deploy",
    "report-deploy": "bash ./scripts/report-deploy.sh",
    "snapshot": "build-storybook && percy-storybook --widths=1280",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@percy/storybook": "^3.3.0",
    "@sebastianwessel/esdoc": "^2.1.0",
    "@sebastianwessel/esdoc-ecmascript-proposal-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-flow-type-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-jsx-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-react-plugin": "^2.1.0",
    "@sebastianwessel/esdoc-standard-plugin": "^2.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/cli": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/react": "^10.4.7",
    "bundlesize": "^0.18.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "flow-bin": "^0.129.0",
    "husky": "^4.2.5",
    "jest-each": "^26.1.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.11",
    "markdownlint-cli": "^0.23.2",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-default": "^3.0.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "engines": {
    "node": "12.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint --fix",
      "flow focus-check",
      "prettier --write"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "package.json": [
      "npmPkgJsonLint -q .",
      "pkg-ok"
    ],
    "README.md": [
      "markdownlint -f"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  }
}
