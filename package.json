{
  "name": "relational-playground",
  "author": "Michael Mior <mmior@cs.rit.edu>",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://relationalplayground.com/",
  "dependencies": {
    "@michaelmior/js-sql-parser": "^1.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "codemirror": "^5.49.2",
    "connected-react-router": "^6.6.1",
    "fast-equals": "^2.0.0",
    "fromentries": "^1.2.0",
    "history": "^4.10.1",
    "prop-types": "^15.5.0",
    "react": "^16.12.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.2.0",
    "react-select": "^3.0.8",
    "react-split-pane": "^0.1.89",
    "react-table": "^6.10.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "*"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "flow": "flow check src",
    "lint": "node_modules/.bin/prettier --check src/**/*.{js,jsx,json} && eslint src",
    "report-deploy": "bash ./scripts/report-deploy.sh",
    "now-build": "REACT_APP_GIT_SHA=`([ -n \"$NOW_GITHUB_COMMIT_SHA\" ] && echo \"$NOW_GITHUB_COMMIT_SHA\") || git rev-parse HEAD` react-scripts build && build-storybook -o build/storybook/ && esdoc && mv docs/ build/ && yarn report-deploy",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "snapshot": "build-storybook && percy-storybook --widths=1280"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@percy/storybook": "^3.2.0",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/cli": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/react": "^9.3.2",
    "babel-loader": "8.0.6",
    "coveralls": "^3.0.9",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "flow-bin": "^0.113.0",
    "husky": "^3.1.0",
    "jest-each": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.5.0",
    "markdownlint-cli": "^0.19.0",
    "npm-package-json-lint": "^4.2.0",
    "npm-package-json-lint-config-default": "^2.0.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "source-map-explorer": "^2.1.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0"
  },
  "engines": {
    "node": "10.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "flow focus-check",
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npmPkgJsonLint -q .",
      "pkg-ok"
    ],
    "README.md": [
       "markdownlint -f"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  }
}
