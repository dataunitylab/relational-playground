{
  "version": 2,
  "builds": [{
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "build"
      }
  }],
  "build": {
    "env": {
      "CI": "1",
      "SENTRY_AUTH_TOKEN": "@sentry_auth_token",
      "SENTRY_ORG": "rit-data-lab"
    }
  },
  "headers": [
    {
      "source": "*",
      "headers": [
        {"key": "X-Content-Type-Options", "value": "nosniff"},
        {"key": "X-Frame-Options", "value": "DENY"},
        {"key": "X-XSS-Protection", "value": "1; mode=block"}
      ]
    }
  ]
}
